{
    "name": "Interclip extension",
    "version": "0.0.4",
    "manifest_version": 2,
    "description": "A chromium extension to interact with the Interclip API.",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "web_accessible_resources": [
        "images/*.jpeg"
    ],
    "permissions": [
        "tabs"
    ]
}